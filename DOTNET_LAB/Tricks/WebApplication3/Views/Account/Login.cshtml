@model WebApplication3.Models.LoginModel

@{
    ViewBag.Title = "Login";
}

<h2>Login</h2>

@using (Html.BeginForm())
{
    <div>
        Username : @Html.TextBoxFor(m => m.Username)
    </div>
    <div>
        Password : @Html.PasswordFor(m => m.Password)
    </div>
    <div>
        <input type="submit" name="name" value="Login"/>
    </div>
}